2,\n        title: 'Hi Nanna',\n        type: 'Movie',\n        description: 'Emotional drama with Nani',\n        image: 'https://i.ibb.co/YRbp6Gg/hi-nanna.jpg',\n        is4K: true\n      }, {\n        id: 3,\n        title: 'Animal',\n        type: 'Movie',\n        description: 'Action thriller',\n        image: 'https://i.ibb.co/wYBqgBF/animal.jpg',\n        is4K: true,\n        isPremium: true\n      }, {\n        id: 4,\n        title: 'Dunki',\n        type: 'Movie',\n        description: 'Comedy drama',\n        image: 'https://i.ibb.co/0QGBHdh/dunki.jpg',\n        is4K: true,\n        isPremium: true\n      }, {\n        id: 5,\n        title: 'Devil',\n        type: 'Movie',\n        description: 'Action thriller',\n        image: 'https://i.ibb.co/Jt8LXYL/devil.jpg',\n        is4K: true,\n        isPremium: true\n      }, {\n        id: 6,\n        title: 'Extra Ordinary Man',\n        type: 'Movie',\n        description: 'Action comedy',\n        image: 'https://i.ibb.co/ZX4Lk8Y/extra-ordinary-man.jpg',\n        is4K: true\n      }, {\n        id: 7,\n        title: 'Eagle',\n        type: 'Movie',\n        description: 'Action thriller',\n        image: 'https://i.ibb.co/M6BMTL6/eagle.jpg',\n        is4K: true,\n        isPremium: true\n      }, {\n        id: 8,\n        title: 'Saindhav',\n        type: 'Movie',\n        description: 'Action drama',\n        image: 'https://i.ibb.co/xGQ8YW4/saindhav.jpg',\n        is4K: true,\n        isPremium: true\n      }]\n    };\n  },\n  mounted() {\n    // Load recent searches from localStorage when component mounts\n    this.loadRecentSearches();\n  },\n  methods: {\n    loadRecentSearches() {\n      const savedSearches = localStorage.getItem('recentSearches');\n      this.recentSearches = savedSearches ? JSON.parse(savedSearches) : [];\n    },\n    saveRecentSearches() {\n      localStorage.setItem('recentSearches', JSON.stringify(this.recentSearches));\n    },\n    addToRecentSearches(query) {\n      if (!query.trim()) return;\n\n      // Remove if already exists (to move it to top)\n      this.recentSearches = this.recentSearches.filter(s => s !== query);\n\n      // Add to beginning of array\n      this.recentSearches.unshift(query);\n\n      // Keep only last 10 searches\n      if (this.recentSearches.length > 10) {\n        this.recentSearches = this.recentSearches.slice(0, 10);\n      }\n\n      // Save to localStorage\n      this.saveRecentSearches();\n    },\n    async handleSearch() {\n      const query = this.searchQuery.trim();\n      if (query) {\n        this.isLoading = true;\n        try {\n          // Add to recent searches\n          this.addToRecentSearches(query);\n\n          // Call backend API for search\n          const response = await fetch(`${process.env.VUE_APP_API_URL}/api/search?q=${encodeURIComponent(query)}`, {\n            method: 'GET',\n            headers: {\n              'Authorization': `Bearer ${localStorage.getItem('token')}`,\n              'Content-Type': 'application/json'\n            }\n          });\n          if (!response.ok) {\n            throw new Error('Search failed');\n          }\n          const data = await response.json();\n          this.searchResults = data.results || [];\n\n          // Update suggestions from API response\n          this.suggestions = data.suggestions || [];\n        } catch (error) {\n          console.error('Search error:', error);\n          this.searchResults = [];\n          this.suggestions = [];\n        }\n      } else {\n        this.searchResults = [];\n        this.suggestions = [];\n      }\n      this.isLoading = false;\n    },\n    updateSuggestions() {\n      // Update search suggestions based on query\n    },\n    clearSearch() {\n      this.searchQuery = '';\n      this.searchResults = [];\n      this.suggestions = [];\n    },\n    goBack() {\n      this.$router.go(-1);\n    },\n    applyFilter(filter) {\n      this.searchQuery = filter;\n      this.handleSearch();\n    },\n    applyRecentSearch(search) {\n      this.searchQuery = search;\n      this.handleSearch();\n    },\n    removeRecentSearch(search) {\n      this.recentSearches = this.recentSearches.filter(s => s !== search);\n      this.saveRecentSearches();\n    },\n    clearRecentSearches() {\n      this.recentSearches = [];\n      localStorage.removeItem('recentSearches');\n    },\n    applySuggestion(suggestion) {\n      this.searchQuery = suggestion;\n      this.handleSearch();\n    },\n    handleTrendingClick(item) {\n      // Navigate based on content type\n      if (item.type === 'Movie') {\n        this.$router.push(`/movie/${item.id}`);\n      } else if (item.type === 'Web Series') {\n        this.$router.push(`/series/${item.id}`);\n      } else {\n        this.$router.push(`/content/${item.id}`);\n      }\n    },\n    handleResultClick(result) {\n      // Navigate based on content type\n      if (result.type === 'Movie') {\n        this.$router.push(`/movie/${result.id}`);\n      } else if (result.type === 'Web Series') {\n        this.$router.push(`/series/${result.id}`);\n      } else {\n        this.$router.push(`/content/${result.id}`);\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","searchQuery","isInputFocused","searchResults","suggestions","recentSearches","isLoading","quickFilters","trendingItems","id","title","type","image","isNewEpisode","is4K","isPremium","sampleResults","description","mounted","loadRecentSearches","methods","savedSearches","localStorage","getItem","JSON","parse","saveRecentSearches","setItem","stringify","addToRecentSearches","query","trim","filter","s","unshift","length","slice","handleSearch","response","fetch","process","env","VUE_APP_API_URL","encodeURIComponent","method","headers","ok","Error","json","results","error","console","updateSuggestions","clearSearch","goBack","$router","go","applyFilter","applyRecentSearch","search","removeRecentSearch","clearRecentSearches","removeItem","applySuggestion","suggestion","handleTrendingClick","item","push","handleResultClick","result"],"sources":["D:\\OTT\\ott_frontend\\src\\views\\SearchPage.vue"],"sourcesContent":["<template>\n  <div class=\"search-page\">\n    <button class=\"back-btn\" @click=\"goBack\" aria-label=\"Go back\">\n      <i class=\"fas fa-arrow-left\"></i>\n    </button>\n    \n    <div class=\"search-container\">\n      <div class=\"search-input-wrapper\" :class=\"{ 'focused': isInputFocused }\">\n        <i class=\"fas fa-search search-icon\"></i>\n        <input \n          type=\"text\" \n          v-model=\"searchQuery\" \n          placeholder=\"Search Title, Movie or Cast\"\n          @input=\"handleSearch\"\n          @focus=\"isInputFocused = true\"\n          @blur=\"isInputFocused = false\"\n          class=\"search-input\"\n          autofocus\n        >\n        <button v-if=\"searchQuery\" @click=\"clearSearch\" class=\"clear-btn\" aria-label=\"Clear search\">\n          <i class=\"fas fa-times\"></i>\n        </button>\n      </div>\n\n      <!-- Quick Filters -->\n      <div class=\"quick-filters\" v-if=\"!searchQuery\">\n        <button \n          v-for=\"filter in quickFilters\" \n          :key=\"filter\"\n          class=\"filter-chip\"\n          @click=\"applyFilter(filter)\"\n        >\n          {{ filter }}\n        </button>\n      </div>\n\n      <!-- Recent Searches -->\n      <div class=\"recent-searches\" v-if=\"!searchQuery && recentSearches.length\">\n        <div class=\"section-header\">\n          <h2 class=\"section-title\">Recent Searches</h2>\n          <button @click=\"clearRecentSearches\" class=\"clear-all-btn\">Clear All</button>\n        </div>\n        <div class=\"recent-list\">\n          <div v-for=\"search in recentSearches\" :key=\"search\" class=\"recent-item\" @click=\"applyRecentSearch(search)\">\n            <i class=\"fas fa-history\"></i>\n            <span>{{ search }}</span>\n            <button @click.stop=\"removeRecentSearch(search)\" class=\"remove-btn\">\n              <i class=\"fas fa-times\"></i>\n            </button>\n          </div>\n        </div>\n      </div>\n\n      <!-- Trending Searches -->\n      <div class=\"trending-section\" v-if=\"!searchQuery\">\n        <h2 class=\"section-title\">Trending Now</h2>\n        <div class=\"trending-grid\">\n          <div v-for=\"item in trendingItems\" :key=\"item.id\" class=\"trending-card\" @click=\"handleTrendingClick(item)\">\n            <div class=\"trending-image\">\n              <img :src=\"item.image\" :alt=\"item.title\">\n              <div class=\"badges\">\n                <span v-if=\"item.isPremium\" class=\"badge premium\">Premium</span>\n              </div>\n            </div>\n            <div class=\"trending-info\">\n              <h3>{{ item.title }}</h3>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- Search Results -->\n      <div class=\"search-results\" v-else>\n        <!-- Search Suggestions -->\n        <div class=\"search-suggestions\" v-if=\"suggestions.length && !searchResults.length\">\n          <div v-for=\"suggestion in suggestions\" \n               :key=\"suggestion\" \n               class=\"suggestion-item\"\n               @click=\"applySuggestion(suggestion)\">\n            <i class=\"fas fa-search\"></i>\n            <span>{{ suggestion }}</span>\n          </div>\n        </div>\n\n        <!-- Results Grid -->\n        <div class=\"results-grid\" v-if=\"searchResults.length\">\n          <div v-for=\"result in searchResults\" \n               :key=\"result.id\" \n               class=\"result-card\"\n               @click=\"handleResultClick(result)\">\n            <div class=\"result-image\">\n              <img :src=\"result.image\" :alt=\"result.title\">\n              <div class=\"badges\">\n                <span v-if=\"result.isPremium\" class=\"badge premium\">Premium</span>\n              </div>\n            </div>\n            <div class=\"result-info\">\n              <h3>{{ result.title }}</h3>\n            </div>\n          </div>\n        </div>\n\n        <!-- No Results -->\n        <div class=\"no-results\" v-else-if=\"!suggestions.length\">\n          <i class=\"fas fa-search no-results-icon\"></i>\n          <p>No results found for \"{{ searchQuery }}\"</p>\n          <p class=\"no-results-suggestion\">Try different keywords or browse trending content</p>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'SearchPage',\n  data() {\n    return {\n      searchQuery: '',\n      isInputFocused: false,\n      searchResults: [],\n      suggestions: [],\n      recentSearches: [],\n      isLoading: false,\n      quickFilters: ['Movies', 'Web Series', 'TV Shows', 'Kids'],\n      trendingItems: [\n        {\n          id: 1,\n          title: 'Mega Grand Finale',\n          type: 'Show',\n          image: 'https://i.ibb.co/Jk3kB0r/mega-finale.jpg',\n          isNewEpisode: true,\n          is4K: true,\n          isPremium: true\n        },\n        {\n          id: 2,\n          title: 'RRR',\n          type: 'Movie',\n          image: 'https://i.ibb.co/VCQRyM1/rrr.jpg',\n          is4K: true,\n          isPremium: true\n        },\n        {\n          id: 3,\n          title: 'Pushpa',\n          type: 'Movie',\n          image: 'https://i.ibb.co/0MKzGTw/pushpa.jpg',\n          is4K: true,\n          isPremium: true\n        },\n        {\n          id: 4,\n          title: 'Guntur Kaaram',\n          type: 'Movie',\n          image: 'https://i.ibb.co/tCW0q0Y/guntur-kaaram.jpg',\n          isNewEpisode: true,\n          is4K: true,\n          isPremium: true\n        },\n        {\n          id: 5,\n          title: 'Waltair Veerayya',\n          type: 'Movie',\n          image: 'https://i.ibb.co/Kj8vXNm/waltair-veerayya.jpg',\n          is4K: true,\n          isPremium: true\n        },\n        {\n          id: 6,\n          title: 'Custody',\n          type: 'Movie',\n          image: 'https://i.ibb.co/9vPNYtL/custody.jpg',\n          isNewEpisode: true,\n          is4K: true\n        }\n      ],\n      // Sample search results that will show when searching\n      sampleResults: [\n        {\n          id: 1,\n          title: 'Salaar',\n          type: 'Movie',\n          description: 'Action drama starring Prabhas',\n          image: 'https://i.ibb.co/Ld4q7Gv/salaar.jpg',\n          is4K: true,\n          isPremium: true\n        },\n        {\n          id: 2,\n          title: 'Hi Nanna',\n          type: 'Movie',\n          description: 'Emotional drama with Nani',\n          image: 'https://i.ibb.co/YRbp6Gg/hi-nanna.jpg',\n          is4K: true\n        },\n        {\n          id: 3,\n          title: 'Animal',\n          type: 'Movie',\n          description: 'Action thriller',\n          image: 'https://i.ibb.co/wYBqgBF/animal.jpg',\n          is4K: true,\n          isPremium: true\n        },\n        {\n          id: 4,\n          title: 'Dunki',\n          type: 'Movie',\n          description: 'Comedy drama',\n          image: 'https://i.ibb.co/0QGBHdh/dunki.jpg',\n          is4K: true,\n          isPremium: true\n        },\n        {\n          id: 5,\n          title: 'Devil',\n          type: 'Movie',\n          description: 'Action thriller',\n          image: 'https://i.ibb.co/Jt8LXYL/devil.jpg',\n          is4K: true,\n          isPremium: true\n        },\n        {\n          id: 6,\n          title: 'Extra Ordinary Man',\n          type: 'Movie',\n          description: 'Action comedy',\n          image: 'https://i.ibb.co/ZX4Lk8Y/extra-ordinary-man.jpg',\n          is4K: true\n        },\n        {\n          id: 7,\n          title: 'Eagle',\n          type: 'Movie',\n          description: 'Action thriller',\n          image: 'https://i.ibb.co/M6BMTL6/eagle.jpg',\n          is4K: true,\n          isPremium: true\n        },\n        {\n          id: 8,\n          title: 'Saindhav',\n          type: 'Movie',\n          description: 'Action drama',\n          image: 'https://i.ibb.co/xGQ8YW4/saindhav.jpg',\n          is4K: true,\n          isPremium: true\n        }\n      ]\n    }\n  },\n  mounted() {\n    // Load recent searches from localStorage when component mounts\n    this.loadRecentSearches()\n  },\n  methods: {\n    loadRecentSearches() {\n      const savedSearches = localStorage.getItem('recentSearches')\n      this.recentSearches = savedSearches ? JSON.parse(savedSearches) : []\n    },\n    saveRecentSearches() {\n      localStorage.setItem('recentSearches', JSON.stringify(this.recentSearches))\n    },\n    addToRecentSearches(query) {\n      if (!query.trim()) return\n      \n      // Remove if already exists (to move it to top)\n      this.recentSearches = this.recentSearches.filter(s => s !== query)\n      \n      // Add to beginning of array\n      this.recentSearches.unshift(query)\n      \n      // Keep only last 10 searches\n      if (this.recentSearches.length > 10) {\n        this.recentSearches = this.recentSearches.slice(0, 10)\n      }\n      \n      // Save to localStorage\n      this.saveRecentSearches()\n    },\n    async handleSearch() {\n      const query = this.searchQuery.trim();\n      if (query) {\n        this.isLoading = true;\n        try {\n          // Add to recent searches\n          this.addToRecentSearches(query)\n          \n          // Call backend API for search\n          const response = await fetch(`${process.env.VUE_APP_API_URL}/api/search?q=${encodeURIComponent(query)}`, {\n            method: 'GET',\n            headers: {\n              'Authorization': `Bearer ${localStorage.getItem('token')}`,\n              'Content-Type': 'application/json'\n            }\n          })\n          \n          if (!response.ok) {\n            throw new Error('Search failed')\n          }\n          \n          const data = await response.json()\n          this.searchResults = data.results || []\n          \n          // Update suggestions from API response\n          this.suggestions = data.suggestions || []\n        } catch (error) {\n          console.error('Search error:', error)\n          this.searchResults = []\n          this.suggestions = [];\n        }\n      } else {\n        this.searchResults = [];\n        this.suggestions = [];\n      }\n      this.isLoading = false;\n    },\n    updateSuggestions() {\n      // Update search suggestions based on query\n    },\n    clearSearch() {\n      this.searchQuery = ''\n      this.searchResults = []\n      this.suggestions = []\n    },\n    goBack() {\n      this.$router.go(-1)\n    },\n    applyFilter(filter) {\n      this.searchQuery = filter\n      this.handleSearch()\n    },\n    applyRecentSearch(search) {\n      this.searchQuery = search\n      this.handleSearch()\n    },\n    removeRecentSearch(search) {\n      this.recentSearches = this.recentSearches.filter(s => s !== search)\n      this.saveRecentSearches()\n    },\n    clearRecentSearches() {\n      this.recentSearches = []\n      localStorage.removeItem('recentSearches')\n    },\n    applySuggestion(suggestion) {\n      this.searchQuery = suggestion\n      this.handleSearch()\n    },\n    handleTrendingClick(item) {\n      // Navigate based on content type\n      if (item.type === 'Movie') {\n        this.$router.push(`/movie/${item.id}`)\n      } else if (item.type === 'Web Series') {\n        this.$router.push(`/series/${item.id}`)\n      } else {\n        this.$router.push(`/content/${item.id}`)\n      }\n    },\n    handleResultClick(result) {\n      // Navigate based on content type\n      if (result.type === 'Movie') {\n        this.$router.push(`/movie/${result.id}`)\n      } else if (result.type === 'Web Series') {\n        this.$router.push(`/series/${result.id}`)\n      } else {\n        this.$router.push(`/content/${result.id}`)\n      }\n    }\n  }\n}\n</script>\n\n<style scoped>\n.search-page {\n  min-height: 100vh;\n  background: linear-gradient(135deg, #121212 0%, #1a1a1a 100