xt\" \n          v-model=\"searchQuery\" \n          placeholder=\"Search Title, Movie or Cast\"\n          @input=\"debouncedSearch\"\n          @focus=\"isInputFocused = true\"\n          @blur=\"isInputFocused = false\"\n          class=\"search-input\"\n          autofocus\n        >\n        <button v-if=\"searchQuery\" @click=\"clearSearch\" class=\"clear-btn\" aria-label=\"Clear search\">\n          <i class=\"fas fa-times\"></i>\n        </button>\n      </div>\n\n      <!-- Quick Filters -->\n      <div class=\"quick-filters\" v-if=\"!searchQuery\">\n        <button \n          v-for=\"filter in quickFilters\" \n          :key=\"filter\"\n          class=\"filter-chip\"\n          @click=\"applyFilter(filter)\"\n        >\n          {{ filter }}\n        </button>\n      </div>\n\n      <!-- Recent Searches -->\n      <div class=\"recent-searches\" v-if=\"!searchQuery && recentSearches.length\">\n        <div class=\"section-header\">\n          <h2 class=\"section-title\">Recent Searches</h2>\n          <button @click=\"clearRecentSearches\" class=\"clear-all-btn\">Clear All</button>\n        </div>\n        <div class=\"recent-list\">\n          <div v-for=\"search in recentSearches\" :key=\"search\" class=\"recent-item\" @click=\"applyRecentSearch(search)\">\n            <i class=\"fas fa-history\"></i>\n            <span>{{ search }}</span>\n            <button @click.stop=\"removeRecentSearch(search)\" class=\"remove-btn\">\n              <i class=\"fas fa-times\"></i>\n            </button>\n          </div>\n        </div>\n      </div>\n\n      <!-- Trending Searches -->\n      <div class=\"trending-section\" v-if=\"!searchQuery\">\n        <h2 class=\"section-title\">Trending Now</h2>\n        <div class=\"trending-grid\">\n          <div v-for=\"item in trendingItems\" :key=\"item.id\" class=\"trending-card\" @click=\"handleTrendingClick(item)\">\n            <div class=\"trending-image\">\n              <img :src=\"item.image\" :alt=\"item.title\">\n              <div class=\"badges\">\n                <span v-if=\"item.isPremium\" class=\"badge premium\">Premium</span>\n              </div>\n            </div>\n            <div class=\"trending-info\">\n              <h3>{{ item.title }}</h3>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- Search Results -->\n      <div class=\"search-results\" v-else>\n        <!-- Loading Indicator -->\n        <div class=\"loading-indicator\" v-if=\"isLoading\">\n          <div class=\"spinner\"></div>\n          <p>Searching...</p>\n        </div>\n\n        <!-- Error Message -->\n        <div class=\"error-message\" v-if=\"searchError && !isLoading\">\n          <i class=\"fas fa-exclamation-circle error-icon\"></i>\n          <p>{{ searchError }}</p>\n          <button @click=\"handleSearch\" class=\"retry-btn\">\n            <i class=\"fas fa-redo\"></i> Retry\n          </button>\n        </div>\n        <!-- Search Suggestions -->\n        <div class=\"search-suggestions\" v-if=\"suggestions.length && !searchResults.length\">\n          <div v-for=\"suggestion in suggestions\" \n               :key=\"suggestion\" \n               class=\"suggestion-item\"\n               @click=\"applySuggestion(suggestion)\">\n            <i class=\"fas fa-search\"></i>\n            <span>{{ suggestion }}</span>\n          </div>\n        </div>\n\n        <!-- Results Grid -->\n        <div class=\"results-grid\" v-if=\"searchResults.length\">\n          <div v-for=\"result in searchResults\" \n               :key=\"result.id\" \n               class=\"result-card\"\n               @click=\"handleResultClick(result)\">\n            <div class=\"result-image\">\n              <img :src=\"result.thumbnail || result.image\" :alt=\"result.title\">\n              <div class=\"badges\">\n                <span v-if=\"result.is_premium\" class=\"badge premium\">Premium</span>\n                <span v-if=\"result.quality\" class=\"badge quality\">{{ result.quality }}</span>\n              </div>\n            </div>\n            <div class=\"result-info\">\n              <h3>{{ result.title }}</h3>\n              <p v-if=\"result.description\" class=\"description\">{{ result.description }}</p>\n              <div class=\"meta-info\">\n                <span v-if=\"result.duration\" class=\"duration\">\n                  <i class=\"fas fa-clock\"></i> {{ result.duration }}\n                </span>\n                <span v-if=\"result.year\" class=\"year\">\n                  <i class=\"fas fa-calendar\"></i> {{ result.year }}\n                </span>\n                <span v-if=\"result.language\" class=\"language\">\n                  <i class=\"fas fa-language\"></i> {{ result.language }}\n                </span>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <!-- No Results -->\n        <div class=\"no-results\" v-else-if=\"!suggestions.length\">\n          <i class=\"fas fa-search no-results-icon\"></i>\n          <p>No results found for \"{{ searchQuery }}\"</p>\n          <p class=\"no-results-suggestion\">Try different keywords or browse trending content</p>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'SearchPage',\n  beforeUnmount() {\n    if (this.searchTimeout) {\n      clearTimeout(this.searchTimeout);\n    }\n  },\n  data() {\n    return {\n      searchQuery: '',\n      isInputFocused: false,\n      searchResults: [],\n      suggestions: [],\n      recentSearches: [],\n      isLoading: false,\n      searchTimeout: null,\n      searchError: null,\n      quickFilters: ['Movies', 'Web Series', 'TV Shows', 'Kids'],\n      trendingItems: [\n        {\n          id: 1,\n          title: 'Mega Grand Finale',\n          type: 'Show',\n          image: 'https://i.ibb.co/Jk3kB0r/mega-finale.jpg',\n          isNewEpisode: true,\n          is4K: true,\n          isPremium: true\n        },\n        {\n          id: 2,\